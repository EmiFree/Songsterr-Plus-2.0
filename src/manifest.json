{

	"manifest_version": 2,
	"name": "Songsterr Plus Hack 2.0",
	"version": "1.1.3",
	"icons": {
		"48": "someicon.png"
	},
	"description": "unlocks songsterr premium cuz fuck them :D",
	"page_action": {
		"default_icon": {
			"16": "someicon.png",
			"32": "someicon.png"
		},
		"default_title": "Hack Premium ;)",
		"show_matches": ["https://www.songsterr.com/*"]
	},
	"content_scripts": [
		{
			"matches": ["https://www.songsterr.com/*"],
			"js": ["inject.js"]
		}
	],
	"background": {
		"scripts": ["block.js"]
	},
	"permissions": [
		"*://www.songsterr.com/*",
		"*://gamergirlandco.github.io/*",
		"webRequest",
		"webRequestBlocking",
		"activeTab",
		"tabs",
		"<all_urls>",
		"idle"
	]

}